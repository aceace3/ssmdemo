<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhoayi.dao.TestControllerDao">

    <select id="findAll" resultType="com.zhoayi.bean.Student">
        select * from student
    </select>

    <select id="findById" resultType="com.zhoayi.bean.Student">
        select * from student where id = #{id}
    </select>
</mapper>